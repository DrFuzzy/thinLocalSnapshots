<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zerowidth.launched.thinlocalsnapshots</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>osascript ~/Desktop/thinLocalSnapshots/thinLocalSnapshots.scpt</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartInterval</key>
	<integer>10800</integer>
	<key>UserName</key>
	<string>USERNAME</string>
	<key>WorkingDirectory</key>
	<string>~/Desktop/thinLocalSnapshots</string>
</dict>
</plist>